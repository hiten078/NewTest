class Solution:
    def constructDistancedSequence(self, n: int) -> List[int]:
        self.n = n
        sequence_length = 2 * n - 1
        self.sequence = [0] * sequence_length
        self.is_used = [False] * (n + 1)
        self.result = None
        
        self._backtrack(0)
        return self.result

    def _backtrack(self, index: int) -> bool:
        if self.result:
            return True

        if index == len(self.sequence):
            self.result = list(self.sequence)
            return True
        
        if self.sequence[index] != 0:
            return self._backtrack(index + 1)
        
        for num_to_place in range(self.n, 0, -1):
            if not self.is_used[num_to_place]:
                if num_to_place == 1:
                    self.sequence[index] = 1
                    self.is_used[1] = True
                    
                    if self._backtrack(index + 1):
                        return True
                    
                    self.sequence[index] = 0
                    self.is_used[1] = False
                else:
                    second_pos = index + num_to_place
                    if second_pos < len(self.sequence) and self.sequence[second_pos] == 0:
                        self.sequence[index] = num_to_place
                        self.sequence[second_pos] = num_to_place
                        self.is_used[num_to_place] = True
                        
                        if self._backtrack(index + 1):
                            return True
                        
                        self.sequence[index] = 0
                        self.sequence[second_pos] = 0
                        self.is_used[num_to_place] = False
        
        return False